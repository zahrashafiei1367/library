<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>serv1</servlet-name>
        <servlet-class>Auotentication</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>serv1</servlet-name>
        <url-pattern>/auotenticate</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>serv2</servlet-name>
        <servlet-class>Registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>serv2</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>serv3</servlet-name>
        <servlet-class>AdminWorks</servlet-class>
        <init-param>
            <param-name>id</param-name>
            <param-value></param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>serv3</servlet-name>
        <url-pattern>/adminJob</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>serv4</servlet-name>
        <servlet-class>SignOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>serv4</servlet-name>
        <url-pattern>/signOut</url-pattern>
    </servlet-mapping>
    <context-param>
        <param-name>username</param-name>
        <param-value></param-value>
    </context-param>
    <context-param>
        <param-name>admin</param-name>
        <param-value></param-value>
    </context-param>
</web-app>